<ul>
    {% for item in menu %}

        <li class="nav-item {% if item|current %}nav-item-active{% endif %} {% if item.class is defined %}{{ item.class }}{% endif %}">

            {% if item.link is defined and item.link is not empty and item.link != "" %}

                <a href="{{ item.link }}" class="nav-link">
                    {{ item.label }} {% if item|current %}<span class="sr-only">(current)</span>{% endif %}
                </a>

            {% endif %}

        </li>

    {% endfor %}
</ul>